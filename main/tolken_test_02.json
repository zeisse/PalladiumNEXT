    "FontSize": {
      "xxs": {
        "value": "{size.global.base} - ( {size.global.minor} * 3) * {size.global.base}",
        "type": "fontSizes"
      },
      "xs": {
        "value": "{size.global.base} - ( {size.global.minor} * 2 ) * {size.global.base}",
        "type": "fontSizes"
      },
      "sm": {
        "value": "{size.global.base} - ( {size.global.minor} * 1 ) * {size.global.base}",
        "type": "fontSizes"
      },
      "md": {
        "value": "{size.global.base} - ( {size.global.minor} * 0 ) * {size.global.base}",
        "type": "fontSizes"
      },
      "lg": {
        "value": "{size.global.base} + ( {size.global.minor} * 2 ) * {size.global.base}",
        "type": "fontSizes"
      },
      "xl": {
        "value": "{size.global.base} + ( {size.global.minor} * 6 ) * {size.global.base}",
        "type": "fontSizes"
      }
    },
    "LineHeight": {
      "xxs": {
        "value": "( ( {size.global.base} - ( {size.global.minor} * 3) * {size.global.base} ) * {size.global.major} )",
        "type": "lineHeights"
      },
      "xs": {
        "value": "( ( {size.global.base} - ( {size.global.minor} * 2) * {size.global.base} ) * {size.global.major} )",
        "type": "lineHeights"
      },
      "sm": {
        "value": "( ( {size.global.base} - ( {size.global.minor} * 1) * {size.global.base} ) * {size.global.major} )",
        "type": "lineHeights"
      },
      "md": {
        "value": "( ( {size.global.base} - ( {size.global.minor} * 0) * {size.global.base} ) * {size.global.major} )",
        "type": "lineHeights"
      },
      "lg": {
        "value": "( ( {size.global.base} + ( {size.global.minor} * 2) * {size.global.base} ) * {size.global.major} )",
        "type": "lineHeights"
      },
      "xl": {
        "value": "( ( {size.global.base} + ( {size.global.minor} * 6) * {size.global.base} ) * {size.global.major} )",
        "type": "lineHeights"
      }
